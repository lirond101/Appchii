<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.myRemax.hibernate_model.CustomersEntity" table="customers" schema="" catalog="ebdb">
        <id name="customersid">
            <column name="customersid" sql-type="int" not-null="true"/>
            <generator class="increment" />
        </id>
        <property name="firstname">
            <column name="firstname" sql-type="varchar" length="45"/>
        </property>
        <property name="lastname">
            <column name="lastname" sql-type="varchar" length="45"/>
        </property>
        <property name="telNum">
            <column name="tel_num" sql-type="varchar" length="45"/>
        </property>
    </class>
</hibernate-mapping>